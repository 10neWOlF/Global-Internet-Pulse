{
  "crons": [
    {
      "path": "/api/cron/daily-update",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/monthly-speed-update",
      "schedule": "0 8 1 * *"
    }
  ],
  "functions": {
    "src/app/api/daily-pulse/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/cron/daily-update/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/monthly-speed-update/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate=300"
        }
      ]
    }
  ]
}
